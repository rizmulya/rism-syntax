{
  "name": "rism-syntax",
  "displayName": "rism syntax",
  "description": "Auto complete and Syntax highlighter for '.rism''",
  "version": "0.0.3",
  "engines": {
    "vscode": "^1.88.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/rizmulya/rism-syntax"
  },
  "icon": "icons/code.png",
  "author": "rizmulya",
  "publisher": "rizmulya",
  "license": "MIT",
  "contributes": {
    "languages": [{
      "id": "rism-syntax",
      "aliases": ["rism", "rism-syntax", "rism-highlighter"],
      "extensions": ["rism"],
      "configuration": "./language-configuration.json",
      "icon": {
        "light": "./icons/copy.png",
        "dark": "./icons/copy.png"
      }
    }],
    "grammars": [{
      "language": "rism-syntax",
      "scopeName": "source.rism",
      "path": "./syntaxes/rism-syntax.tmLanguage.json"
    }]
  }
}
