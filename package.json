{
  "name": "rism-syntax",
  "displayName": "rism syntax",
  "description": "Systax highlighter for '.rism'",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.88.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/rizmulya/rism-syntax"
  },
  "icon": "icons/code.png",
  "author": "rizmulya",
  "publisher": "rizmulya",
  "license": "MIT",
  "contributes": {
    "languages": [{
      "id": "rism-syntax",
      "aliases": ["rism", "rism-syntax", "rism-highlighter"],
      "extensions": ["rism"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "rism-syntax",
      "scopeName": "source.rism",
      "path": "./syntaxes/rism-syntax.tmLanguage.json"
    }]
  }
}
